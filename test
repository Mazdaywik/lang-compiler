
int func(int n) {
   return n == 0 ? 0 : n==1 ? 1 : func(n-2) + func(n-1);
}

void print_num(int n) {
    while(n != 0) {
        putchar(48 + n % 10);
        n = n/10;
    }
}

int main(int a, int b) {
    for(int i = 0; i < 40; i = i + 1) {
        int n = func(i);
        print_num(i);
        putchar(32);
        print_num(n);
        putchar(13);
    }
    return 0;
}